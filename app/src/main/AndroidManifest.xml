<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vivianle.diningtogo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainPageActivity" />
        <activity android:name=".GrilledMenuActivity" />
        <activity android:name=".SaladMenuActivity" />
        <activity android:name=".CharbroilMenuActivity" />
        <activity android:name=".SpecialMenuActivity" />
        <activity android:name=".BalanceActivity" />
        <activity android:name=".CartActivity"></activity>
    </application>

</manifest>